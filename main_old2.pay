import os, json, time
import runpod

print("BOOT: starting container...")

def handler(event):
    print("HANDLER: got event", json.dumps(event)[:200])
    # 何もせずに応答
    return {"ok": True, "echo": event.get("input", {}), "ts": time.time()}

if __name__ == "__main__":
    print("BOOT: calling runpod.serverless.start")
    runpod.serverless.start({"handler": handler})
